    "mini-css-extract-plugin": "^2.7.6",
    "prettier": "^2.8.8",
    "prettier-plugin-import-sort": "^0.0.7",
    "sass": "^1.63.4",
    "sass-loader": "^13.3.2",
    "webpack": "^5.87.0",
    "webpack-bundle-tracker": "2.0.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "prettier": {
    "semi": false,
